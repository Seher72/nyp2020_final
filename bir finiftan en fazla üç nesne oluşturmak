class B {
    static int sayac = 1;

    private B() {//doğrudan nesne oluşturması engelemek için private ekledik

    }

    static B get_nesne() {
        if (sayac <= 3) {//eğer hoca tek bir nesne dese burası <=1 yapıyoruz  
            sayac++;
         return new B();
         
        }
        else{
        return null;
        }
    }

}

public class JavaApplication6 {

    public static void main(String[] args) {
       
    }
}
